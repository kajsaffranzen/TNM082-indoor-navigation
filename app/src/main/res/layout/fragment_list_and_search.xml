<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.firebazzze.tnm082_indoor_navigation.CreatePOIFragment">

    <ListView
        android:id="@+id/listSearch"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginLeft="30dp"
        android:layout_marginRight="30dp"
        android:visibility="gone">
    </ListView>

    <ProgressBar
        android:id="@+id/loadingPanel"
        android:layout_width="92dp"
        android:layout_height="92dp"
        android:layout_gravity="center"
        android:indeterminate="true"
        android:visibility="invisible"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <TextView
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:text="inget hus skannat"
            android:textSize="32dp"
            android:textColor="@color/infoTxt"
            android:gravity="center"
            android:background="@color/backgroundTitle"
            android:textAllCaps="true"
            android:id="@+id/infoText"/>

        <ExpandableListView
            android:id="@+id/expList"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:childDivider="#1111"/>
    </LinearLayout>

    <Button
        android:id="@+id/buttoncreatepoi"
        android:layout_width="@dimen/btn_size_big"
        android:layout_height="@dimen/btn_size_big"
        android:background="@drawable/ic_add"
        android:width="@dimen/btn_size_big"
        android:layout_gravity="center_horizontal|bottom"
        android:layout_marginBottom="15dp"/>
</FrameLayout>
